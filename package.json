{
  "name": "preact-mdl-example",
  "version": "0.1.0",
  "description": "Example using preact-mdl with TypeScript and Webpack",
  "author": "Trevor Bekolay",
  "license": "MIT",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tbekolay/preact-mdl-example.git"
  },
  "bugs": {
    "url": "https://github.com/tbekolay/preact-mdl-example"
  },
  "private": true,
  "scripts": {
    "build": "webpack --debug --devtool source-map --output-pathinfo",
    "debug": "webpack-dev-server --inline --debug --devtool source-map --output-pathinfo",
    "release": "webpack --optimize-minimize --optimize-occurence-order",
    "style:css": "stylelint src/**/*.css",
    "style:ts": "tslint src/**/*.ts*",
    "style": "npm run style:ts && npm run style:css"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "dependencies": {
    "autoprefixer": "^8.4.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.2.1",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "preact": "^8.2.9",
    "preact-mdl": "^2.2.2",
    "preact-router": "^2.3.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.0.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
